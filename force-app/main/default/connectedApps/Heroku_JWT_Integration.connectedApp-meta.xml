<?xml version="1.0" encoding="UTF-8"?>
<ConnectedApp xmlns="http://soap.sforce.com/2006/04/metadata">
    <contactEmail>svetlana.c.dj@gmail.com</contactEmail>
    <label>Heroku JWT Integration</label>
    <oauthConfig>
        <callbackUrl>https://login.salesforce.com/services/oauth2/success</callbackUrl>
        <certificate></certificate>
        <consumerKey>REDACTED_FOR_SECURITY</consumerKey>
